{
  "extends": [
    "prettier",
    "prettier/react",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "react-app"
  ],
  "plugins": [
    "react", "import", "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
      "prettier/prettier": ["error", {
        "useTabs": true,
        "tabWidth": 2,
        "singleQuote": true
      }],
      "comma-dangle": [
        "error", "never"
      ],
      "no-underscore-dangle":"off",
      "react/forbid-prop-types": 0,
      "jsx-a11y/anchor-is-valid": [ 0, {
        "components": [ "Link" ],
        "specialLink": [ "hrefLeft", "hrefRight" ],
        "aspects": [ "noHref", "invalidHref", "preferButton" ]
      }],
    "no-debugger": "off",
    "no-console": "off",
    "react/prop-types": "off",
    "jsx-a11y/href-no-hash": "off",
    "no-unused-vars": [
      "error",
      {
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ]
  }
}